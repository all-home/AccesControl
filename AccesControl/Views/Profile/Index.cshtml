@model IEnumerable<ProfilesDB.Model.Profile>

<div class="row justify-content-lg-center" style="background-color: white; margin-top: 5%">
<p>
    <a asp-action="Create">Добавить новый</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
               Название
            </th>
            <th>
               Время начала работы
            </th>
            <th>
                Время завершения работы
            </th>
            <th>
                Активный
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartWorking)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndWorking)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Изм.</a> 
                <a asp-action="Delete" asp-route-id="@item.id">Удал.</a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>
